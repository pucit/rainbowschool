@model IEnumerable<School_Management_System.Models.subject>

@{
    
    Layout = "../Shared/Layout_1.cshtml";
    
    }

@{
    ViewBag.Title = "SubjectIndex";
}

<h2>SubjectIndex</h2>
<br /><br />
<p>
    @Html.ActionLink("Create New", "SubjectCreate","Result")
</p>
<table>
    <tr>
         <th>
            Subject ID
        </th>
        <th>
            Subject Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.subjectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.subjectName)
        </td>
        <td>
            @Html.ActionLink("Edit", "SubjectEdit", new { id=item.subjectId }) |
            @Html.ActionLink("Delete", "SubjectDelete", new { id=item.subjectId })
        </td>
    </tr>
}

</table>
<br/><br/><br/>
